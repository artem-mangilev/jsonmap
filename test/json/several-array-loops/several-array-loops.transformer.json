{
    "iteration": {
        "#loop($.numbers)": {
            "CurrentValue": "#currentvalue()",
            "CurrentIndex": "#currentindex()",
            "LastValue": "#lastvalue()",
            "LastIndex": "#lastindex()"
        }
    },
    "letterIteration": {
        "#loop($.letters)": {
            "CurrentValue": "#currentvalue()"
        }
    },
    "objects": {
        "#loop($.objects)": "#currentvalueatpath($.country)"
    }
}